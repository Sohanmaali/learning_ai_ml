    "import pdfplumber as pdfp\n",
    "\n",
    "with pdfp.open(\"../1.pdf\") as pdf:\n",
    "    full_text = \"\";\n",
    "    for page in pdf.pages:\n",
    "        full_text += page.extract_text()\n",
    "    with open(\"output.txt\",\"w\",encoding = \"utf-8\") as f:\n",
    "        f.write(full_text)\n",
    "        "
   ]
